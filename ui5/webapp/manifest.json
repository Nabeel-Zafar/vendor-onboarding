{
  "_version": "1.59.0",
  "sap.app": {
    "id": "vendor.onboarding",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "Vendor Onboarding Portal",
    "description": "Vendor Onboarding & Management System"
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "vendor.onboarding.view.App",
      "type": "XML",
      "async": true,
      "id": "appView"
    },
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.f": {},
        "sap.tnt": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "vendor.onboarding.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "vendor.onboarding.view",
        "controlId": "appContainer",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "login",
          "target": "login"
        },
        {
          "pattern": "dashboard",
          "name": "dashboard",
          "target": "dashboard"
        },
        {
          "pattern": "onboarding",
          "name": "onboarding",
          "target": "onboarding"
        },
        {
          "pattern": "vendor/{vendorId}",
          "name": "vendorDetail",
          "target": "vendorDetail"
        },
        {
          "pattern": "users",
          "name": "users",
          "target": "users"
        },
        {
          "pattern": "users/new",
          "name": "userForm",
          "target": "userForm"
        }
      ],
      "targets": {
        "login": {
          "viewName": "Login",
          "viewLevel": 1
        },
        "dashboard": {
          "viewName": "Dashboard",
          "viewLevel": 2
        },
        "onboarding": {
          "viewName": "OnboardingForm",
          "viewLevel": 2
        },
        "vendorDetail": {
          "viewName": "VendorDetail",
          "viewLevel": 3
        },
        "users": {
          "viewName": "UsersPage",
          "viewLevel": 2
        },
        "userForm": {
          "viewName": "UserForm",
          "viewLevel": 3
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
